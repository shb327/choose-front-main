<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.Choose" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">#205072</item>
        <item name="colorPrimaryVariant">#CDE0C9</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorSecondary">#329D9C</item>
        <item name="colorSecondaryVariant">#329D9C</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>

    <style name="Myslider" parent="@style/Widget.MaterialComponents.Slider">
        <item name="labelStyle">@style/My_Tooltip</item>
        <item name="materialThemeOverlay">@style/ThemeOverlay.Slider</item>
    </style>

    <style name="My_Tooltip" parent="Widget.MaterialComponents.Tooltip">
        <item name="backgroundTint">#E0ECDE</item>
    </style>

    <style name="ThemeOverlay.Slider" parent="">
        <item name="colorOnPrimary">#68B2A0</item>
    </style>

    <style name="error_appearance_one" parent="@android:style/TextAppearance">
        <item name="android:textColor">@android:color/transparent</item>
    </style>

    <style name="error_appearance_two" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/error</item>
    </style>

    <style name="Theme.Choose.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="TextInputLayoutStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="boxStrokeColor">#56C596</item>
        <item name="boxStrokeWidth">2dp</item>
    </style>

    <style name="Theme.Choose.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Choose.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Theme.ThemeOne" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">#205072</item>
        <item name="colorPrimaryVariant">@color/white</item>
        <item name="colorOnPrimary">#329D9C</item>
        <item name="colorSecondary">#329D9C</item>
        <item name="colorSecondaryVariant">#329D9C</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>

    <style name="Theme.ThemeTwo" parent="Theme.MaterialComponents.Light">
        <item name="colorPrimary">#CDE0C9</item>
        <item name="colorPrimaryVariant">@color/white</item>
        <item name="colorOnPrimary">#329D9C</item>
        <item name="colorSecondary">#329D9C</item>
        <item name="colorSecondaryVariant">#329D9C</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
    </style>

    <style name="Widget.App.Button.OutlinedButton.Icon" parent="Widget.MaterialComponents.Button.OutlinedButton.Icon">
        <item name="android:padding">0dp</item>
        <item name="iconPadding">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:insetLeft">0dp</item>
        <item name="android:insetRight">0dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.MyApp.Button.Circle" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="MontserratBoldTextAppearance">
        <item name="android:fontFamily">@font/montserrat_bold</item>
    </style>
</resources>